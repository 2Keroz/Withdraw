model Paris {
  id             Int                @id @default(autoincrement())
  utilisateur_id Int
  paris_id       Int?
  equipe_choisie String
  points_mises   Int
  date_pari      DateTime
  perte_id       Int?
  status         String
  matchId        Int

  utilisateur    Utilisateur        @relation("UtilisateurParis", fields: [utilisateur_id], references: [id], onDelete: Cascade)
  match          Match              @relation("MatchParis", fields: [matchId], references: [id])
}